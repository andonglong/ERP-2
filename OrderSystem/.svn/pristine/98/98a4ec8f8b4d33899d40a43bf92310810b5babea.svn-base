rt default {\r\n  data () {\r\n    return {\r\n      form: {\r\n        companyid: '',\r\n        factoryid: '',\r\n        willorderid: '',\r\n        quotationid: '',\r\n        status: '',\r\n        inquiryData: '',\r\n        endinquirytime: null,\r\n        startinquirytime: null,\r\n        engineer: '',\r\n        buyer: '',\r\n        productmodel: '',\r\n        pageindex: 1,\r\n        pagesize: 10\r\n      },\r\n      statuOptions: [{\r\n        value: '',\r\n        label: '全部'\r\n      }, {\r\n        value: '0',\r\n        label: '待报价'\r\n      }, {\r\n        value: '1',\r\n        label: '已向供应商询价'\r\n      }, {\r\n        value: '2',\r\n        label: '供应商已报价'\r\n      }, {\r\n        value: '3',\r\n        label: '已筛选供应商报价'\r\n      }, {\r\n        value: '4',\r\n        label: '已确认建议售价'\r\n      }, {\r\n        value: '5',\r\n        label: '已作废'\r\n      }],\r\n      show: false,\r\n      activeName: 'first',\r\n\r\n      tableData: [],\r\n      filters: [{\r\n        value: '0',\r\n        text: '待报价'\r\n      }, {\r\n        value: '1',\r\n        text: '已向供应商询价'\r\n      }, {\r\n        value: '2',\r\n        text: '供应商已报价'\r\n      }, {\r\n        value: '3',\r\n        text: '已筛选供应商报价'\r\n      }, {\r\n        value: '4',\r\n        text: '已确认建议售价'\r\n      }, {\r\n        value: '5',\r\n        text: '已作废'\r\n      }],\r\n      filters2: [],\r\n      defaultSort: {\r\n        prop: 'date',\r\n        order: 'descending'\r\n      },\r\n      count1: 0,\r\n      count2: 0,\r\n\r\n      listLoading: false,\r\n      multipleSelection: [],\r\n\r\n      // 分页\r\n      page_sizes: [10, 20, 30, 50, 100, 200, 500, 1000],\r\n      Total: 100, // 总条数\r\n\r\n      dialogVisible: false,\r\n      iconTitle: true, // 提醒供应商报价返回状态\r\n      quoteDetail: false\r\n    }\r\n  },\r\n  methods: {\r\n    inquiryDataChange (val) {\r\n      if (!$util.checkDateDaterange(val)) {\r\n        this.$notify({\r\n          title: '警告',\r\n          message: '日期格式错误，请重新输入',\r\n          type: 'warning',\r\n          duration: 1500\r\n        })\r\n        return\r\n      }\r\n      let data = val.split(' - ')\r\n      this.form.startinquirytime = data[0]\r\n      this.form.endinquirytime = data[1]\r\n    },\r\n    // 查询\r\n    search () {\r\n      this.getWillorders(this.form)\r\n    },\r\n    // 重置\r\n    resetForm (formName) {\r\n      this.$refs[formName].resetFields()\r\n    },\r\n    // 导航切换\r\n    handleClick (tab, event) {\r\n      this.$refs['form'].resetFields()\r\n      if (tab.index === 0) {\r\n        this.form.status = ''\r\n      } else if (tab.index === 1) {\r\n        this.form.status = 0\r\n      } else {\r\n        this.form.status = 1\r\n      }\r\n      this.getWillorders(this.form)\r\n    },\r\n    handleSelectionChange (val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    filterTag (value, row) {\r\n      return row.status === value\r\n    },\r\n    filterTag2 (value, row) {\r\n      return row.engineer === value\r\n    },\r\n    toquotationDetail (id) {\r\n      this.$router.push({\r\n        path: '/Quotation/quotationDetail/' + id\r\n      })\r\n    },\r\n    selectable (row, index) {\r\n      return row.statu !== '已作废'\r\n    },\r\n    // 刷新\r\n    handRefresh () {\r\n      let parms = {\r\n        companyid: this.form.companyid,\r\n        factoryid: this.form.factoryid\r\n      }\r\n      this.getWillorders(parms)\r\n    },\r\n    // 提醒供应商报价\r\n    handTipSupplier () {\r\n      if (this.multipleSelection.length < 1) {\r\n        this.$notify({\r\n          title: '温馨提示',\r\n          message: '请勾选报价需求单',\r\n          type: 'warning',\r\n          duration: 1500,\r\n          offset: 60\r\n        })\r\n      } else {\r\n        this.dialogVisible = true\r\n        this.getWillorders(this.form)\r\n      }\r\n      // this.dialogVisible = true\r\n    },\r\n    // 列表区\r\n    tableRowClassName (row, index) {\r\n      if (row.statu === '已作废') {\r\n        return 'info-row-color