  break\r\n          case '2':\r\n            statusTxt = '供应商已报价'\r\n            break\r\n          case '3':\r\n            statusTxt = '已筛选供应商报价'\r\n            break\r\n          case '4':\r\n            statusTxt = '已确认建议售价'\r\n            break\r\n          case '5':\r\n            statusTxt = '已作废'\r\n            break\r\n        }\r\n        value.statusTxt = statusTxt\r\n      })\r\n      personArr = $util.arrayToHeavy(personArr) // 名字去重\r\n      personArr = this.check_person(personArr) // 格式化名字\r\n      return {\r\n        tableData: data,\r\n        personArr: personArr\r\n      }\r\n    },\r\n    check_person (arr) {\r\n      let newArr = []\r\n      arr.map((value, index, arr) => {\r\n        newArr.push({\r\n          text: value,\r\n          value: value\r\n        })\r\n      })\r\n      return newArr\r\n    }\r\n  },\r\n  computed: {\r\n  },\r\n  created () {\r\n    let userInfo = JSON.parse(sessionStorage.getItem('userInfo'))\r\n    if (userInfo == null) {\r\n      this.$router.push('/login')\r\n    }\r\n    this.form.companyid = userInfo.companyid\r\n    this.form.factoryid = userInfo.factoryid\r\n    let parms = {\r\n      companyid: userInfo.companyid,\r\n      factoryid: userInfo.factoryid\r\n    }\r\n    this.getWillorders(parms)\r\n    this.$store.state.adminleftnavnum = '/Quotation/quotation'\r\n  }\r\n}\n</script>\r\n\r\n<style lang=\"stylus\">\r\n\t@import '~common/style/headSearch.styl'\r\n\t.quotation\r\n\t\tbackground-color:#fff\r\n\t\tmargin-left:46px\r\n\t\t.search-box\r\n\t\t\tmin-width:1450px\r\n\t\t\tpadding-top:40px\r\n\t\t\t.form-wrap .search-left .search-item.el-form-item\r\n\t\t\t\tmargin-right:0px\r\n\t\t\t.form-wrap\r\n\t\t\t\toverflow:hidden\r\n\t\t\t\tfont-size:0\r\n\t\t\t\t.search-left.search-left-w\r\n\t\t\t\t\twidth:1140px\r\n\t\t\t\t.search-left\r\n\t\t\t\t\tdisplay:inline-block\r\n\t\t\t\t\tvertical-align: middle\r\n\t\t\t\t\twidth:750px\r\n\t\t\t\t\tmargin-left:0\r\n\t\t\t\t\t.el-form-item__label\r\n\t\t\t\t\t\tcolor