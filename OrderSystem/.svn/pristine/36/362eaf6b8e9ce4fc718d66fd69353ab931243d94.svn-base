yData.custtradesid' placeholder='请选择' class='info-select'>\r\n                <el-option v-for='item in options.IndustryInfo' :key='item.id' :label='item.name' :value='item.id'>\r\n                </el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item label='行业标杆'  class='info-textarea' prop=\"contacttarget\">\r\n              <el-input type='textarea' v-model='IndustryData.contacttarget'  placeholder='多个标杆企业时，用，隔开' :autosize=\"{ minRows: 1, maxRows: 4}\" style='width: 260px'></el-input>\r\n            </el-form-item>\r\n          </div>\r\n          <div class=\"h55\">\r\n            <el-form-item label='主营产品' class=\"w442\" prop='mainproduct'>\r\n              <el-input type='textarea' v-model='IndustryData.mainproduct' placeholder='多个主营产品时，用，隔开'  :autosize=\"{ minRows: 1, maxRows: 4}\" class='info-textarea' style='width: 260px'></el-input>\r\n            </el-form-item>\r\n            <el-form-item label='竞争对手' prop='competitor'>\r\n              <el-input type='textarea' v-model='IndustryData.competitor'  placeholder='多个竞争对手时，用，隔开' :autosize=\"{ minRows: 1, maxRows: 4}\" class='info-textarea' style='width: 260px'></el-input>\r\n            </el-form-item>\r\n          </div>\r\n\r\n          <el-form-item>\r\n            <el-button type='primary' @click='SaveIndustryInfo'>保存</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n    </div>\r\n    <div class='supplier-info-box'>\r\n      <h3 ref='business'>商务信息</h3>\r\n      <div class='supplier-warper'>\r\n        <v-Business :BusinessData='BusinessData' :BusinessOptions='BusinessOptions'></v-Business>\r\n      </div>\r\n    </div>\r\n    <div class='supplier-info-box'>\r\n      <h3 ref='financial'>业务信息</h3>\r\n      <div class='supplier-warper'>\r\n        <v-Financial :custbusinesslist='custbusinesslist'></v-Financial>\r\n      </div>\r\n    </div>\r\n    <div class='supplier-info-box'>\r\n      <h3 ref='linkname'>联系人</h3>\r\n      <div