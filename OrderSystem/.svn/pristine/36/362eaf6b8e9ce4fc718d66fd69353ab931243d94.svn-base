native=\"KeydownNumber($event)\" class=\"w200 mg-lr10\"></el-input>元   \r\n            \r\n               <!-- <el-input-number size=\"small\" v-model=\"tableData[pricenotaxIndex].materialcost\" :min='0' :max=\"9999\" :debounce=\"1000\" class=\"w200 mg-lr10\"></el-input-number>元   -->\r\n              <!-- <input type=\"text\"  placeholder=\"请输入单个产品的表面处理费\" @blur=\"BlurMaterialcost($event,pricenotaxIndex)\"  @keydown=\"KeydownNumber($event)\" class=\"w260 mg-lr10 el-input__inner\"/>元   -->\r\n             </div> \r\n          <div class=\"mg-b10\">\r\n            <span class=\"inblock w90 text-right\">加工费</span>\r\n               <el-input size=\"small\" v-model=\"tableData[pricenotaxIndex].workcost\" @blur=\"BlurWorkcost($event,pricenotaxIndex)\" @keydown.native=\"KeydownNumber($event)\" class=\"w200 mg-lr10\"></el-input>元   \r\n            \r\n             <!-- <el-input-number size=\"small\" v-model=\"tableData[pricenotaxIndex].workcost\" :min='0' :max=\"9999\" :debounce=\"1000\" class=\"w200 mg-lr10\"></el-input-number>元  -->\r\n              <!-- <input type=\"text\"  placeholder=\"请输入单个产品的表面处理费\" @blur=\"BlurWorkcost($event,pricenotaxIndex)\"  @keydown=\"KeydownNumber($event)\" class=\"w260 mg-lr10 el-input__inner\"/>元   -->\r\n            </div>\r\n          <div>\r\n            <span class=\"inblock w90 text-right\">表面处理费</span>\r\n                <el-input size=\"small\" v-model=\"tableData[pricenotaxIndex].dealwithcost\" @blur=\"BlurDealwithcost($event,pricenotaxIndex)\" @keydown.native=\"KeydownNumber($event)\" class=\"w200 mg-lr10\"></el-input>元    \r\n            \r\n              <!-- <el-input-number size=\"small\" v-model=\"tableData[pricenotaxIndex].dealwithcost\" :min='0' :max=\"9999\" :debounce=\"1000\" class=\"w200 mg-lr10\"></el-input-number>元  -->\r\n              <!-- <input type=\"text\"  placeholder=\"请输入单个产品的表面处理费\" @blur=\"BlurDealwithcost($event,pricenotaxIndex)\"  @keydown=\"KeydownNumber($event)\" class=\"w260 mg-lr10 el-input__inner\"/>元   -->\r\n            </div>\r\n        </div>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"ClickDialogConfirm\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </transition>  \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport $http from '../../../api/http.js'\r\nimport api from 'api/request.js'\r\nimport $util from 'common/js/util.js'\r\nimport window from './enquirySheetDetailWindow.vue'\r\nimport importBox from './importBox.vue'\r\nexport default {\r\n  // props: ['windowShow'],\r\n  components: {\r\n    'v-window': window,\r\n    'v-importBox': importBox\r\n  },\r\n  data() {\r\n    return {\r\n      a: '嗨',\r\n      InfoShow: true,\r\n      isShowImport: false,\r\n      baseData: {},\r\n      activeName: 'first',\r\n      windowShow: false,\r\n      tableDataIndex: 0,\r\n      tableData: [],\r\n      options: [{\r\n        text: '13%',\r\n        value: ''\r\n      }],\r\n      dialogVisible: false,\r\n      dialogTitle: '',\r\n      dialogData: {},\r\n      pricenotaxIndex: 0,  // 未税单价index\r\n      isDeliverytime: true,\r\n      isImportPricenotax: true,\r\n\r\n      throttles: {\r\n        refresh: true,\r\n        save: true,\r\n        quoted: true,\r\n        DownDrawing: true\r\n      }\r\n    }\r\n  },\r\n  beforeCreate() { },\r\n  created() {\r\n    this.$store.state.adminleftnavnum = '/EnquirySheet/enquirySheet'\r\n    this.fetchData()\r\n  },\r\n  beforeUpdate() { },\r\n  updated() { },\r\n  beforeMount() { },\r\n  mounted() {\r\n    if (!parseInt(this.$route.query)) {\r\n      let param = this.$route.params\r\n      this.$api.inquiryorder.markreadbyid(param).then(res => {\r\n        if (res.data.code != 200) {\r\n          this.$message.error('标记已读失败')\r\n        }\r\n      })\r\n    }\r\n  },\r\n  filters: {\r\n    toFixed2(val) {\r\n      return parseFloat(val ? val : 0).toFixed(2)\r\n    }\r\n  },\r\n  methods: {\r\n    ClickRefresh() {\r\n      if (!this.throttles.refresh) { return }\r\n      this.throttles.refresh = false\r\n      this.$message.closeAll()\r\n\r\n      this.fetchData({ refresh: true })\r\n    },\r\n    ClickQuoted() {\r\n      if (!this.throttles.quoted) { return }\r\n      this.throttles.quoted = false\r\n      this.$message.closeAll()\r\n\r\n      let param = { inquiryorderid: this.$route.params.inquiryorderid }\r\n      api.inquiryorder.confirmwillorder(param).then(res => {\r\n        if (res.data.code == 200) {\r\n          this.$message.success('报价成功')\r\n        } else {\r\n          this.$message.error(res.data.message)\r\n        }\r\n        this.throttles.quoted = true\r\n      })\r\n    },\r\n    ClickSave() {\r\n      if (!this.throttles.save) { return }\r\n      this.throttles.save = false\r\n      this.$message.closeAll()\r\n\r\n      let copeData = this.tableData\r\n      // 判断是否符合保存条件\r\n      for (let i = 0; i < copeData.length; i++) {\r\n        if (copeData[i].iscando) {\r\n          if (!copeData[i].pricenotax) {\r\n            if (!copeData[i].deliverytime || !copeData[i].pricewithtax) {\r\n              this.$message.error('能做的产品，请您填写货期和含税单价')\r\n              this.throttles.save = true\r\n              return\r\n            }\r\n          } else {\r\n            if (!copeData[i].deliverytime) {\r\n              this.$message.error('能做的产品，请您填写货期')\r\n              this.throttles.save = true\r\n              return\r\n            }\r\n          }\r\n        }\r\n      }\r\n      // 深层拷贝\r\n      copeData = []\r\n      for (let i = 0; i < this.tableData.length; i++) {\r\n        if (typeof this.tableData[i] === 'object') {\r\n          copeData[i] = {}\r\n          for (let j in this.tableData[i]) {\r\n            if (j == 'iscando') {\r\n              copeData[i][j] = this.tableData[i][j] ? '1' : '0'\r\n            } else if (j == 'issendcost') {\r\n              copeData[i][j] = this.tableData[i][j] ? 1 : 0\r\n            } else {\r\n              copeData[i][j] = this.tableData[i][j]\r\n            }\r\n          }\r\n        }\r\n      }\r\n      let param = {\r\n        inquiryorderid: this.$route.params.inquiryorderid,\r\n        detailList: copeData\r\n      }\r\n      api.inquiryorder.updateinquiryorder(param).then(res => {\r\n        if (res.data.code == 200) {\r\n          this.fetchData()\r\n          this.$message.success('保存成功')\r\n        } else {\r\n          this.$message.error(res.data.message)\r\n        }\r\n        this.throttles.save = true\r\n      })\r\n    },\r\n    ClickDownDrawing() {\r\n      if (!this.throttles.DownDrawing) { return }\r\n      this.throttles.DownDrawing = false\r\n      this.$message.closeAll()\r\n\r\n      let param = { inquiryorderid: this.$route.params.inquiryorderid }\r\n      // let param = { inquiryorderid: 201708041472 }\r\n      api.inquiryorder.downloadattachment(param).then(res => {\r\n        if (res.data.code == 200) {\r\n          // location.href = res.data.data\r\n          $http.get(res.data.data)\r\n        } else {\r\n          this.$message.error(res.data.message ? res.data.message : '服务器错误')\r\n        }\r\n        this.throttles.DownDrawing = true\r\n      })\r\n    },\r\n    ClickProductcode(index) {\r\n      this.windowShow = true\r\n      this.tableDataIndex = index\r\n    },\r\n    checkedStatu(satau) {\r\n      if (satau === '0') {\r\n        return '待报价'\r\n      } else if (satau === '1') {\r\n        return '已报价'\r\n      } else if (satau === '2') {\r\n        return '已作废'\r\n      }\r\n    },\r\n    handleClick() { },\r\n    ClickPricenotax(row, index) {\r\n      // this.dialogTitle = row.productcode\r\n      // this.dialogData = row\r\n      this.dialogVisible = true\r\n      this.pricenotaxIndex = index\r\n    },\r\n    ClickDialogConfirm() {\r\n      let data = this.tableData\r\n      let index = this.pricenotaxIndex\r\n      data[index].pricenotax = (parseFloat(data[index].materialcost) + parseFloat(data[index].workcost) + parseFloat(data[index].dealwithcost)).toFixed(2)\r\n      this.dialogVisible = false\r\n    },\r\n    // 是否输入货期\r\n    isImportDeliverytime(flag) {\r\n      this.isDeliverytime = flag\r\n      return flag\r\n    },\r\n    // 是否输入未税单价\r\n    isImportPricewithtaxFnc(flag) {\r\n      this.isImportPricenotax = flag\r\n      return flag\r\n    },\r\n    handleClose() {\r\n      this.dialogVisible = false\r\n    },\r\n    handleSelectionChange() {\r\n    },\r\n    fetchData(obj) {\r\n      let param = { inquiryorderid: this.$route.params.inquiryorderid }\r\n      if (obj && obj.data) {\r\n        param = obj.data\r\n      }\r\n      api.inquiryorder.getinquiryorderbyid(param).then(res => {\r\n        if (res.data.code == 200 && res.data.data) {\r\n          let data = res.data.data\r\n          data.creattime = data.creattime ? $util.DateFormat(data.creattime, 'yyyy-MM-dd  hh:mm:ss') : '--'\r\n          data.asktime = data.asktime ? $util.DateFormat(data.asktime, 'yyyy-MM-dd  hh:mm:ss') : '--'\r\n          data.facktime = data.facktime ? $util.DateFormat(data.facktime, 'yyyy-MM-dd  hh:mm:ss') : '--'\r\n          data.remindtime = data.remindtime ? $util.DateFormat(data.remindtime, 'yyyy-MM-dd  hh:mm:ss') : '--'\r\n          this.baseData = data\r\n          if (obj && obj.method) {\r\n            obj.method()\r\n          }\r\n          for (let i = 0; i < data.detailList.length; i++) {\r\n            data.detailList[i].iscando = data.detailList[i].iscando == '1' ? true : false\r\n            data.detailList[i].issendcost = data.detailList[i].issendcost == '1' ? true : false\r\n            data.detailList[i].materialcost = data.detailList[i].materialcost ? data.detailList[i].materialcost.toFixed(2) : '0.00'\r\n            data.detailList[i].workcost = data.detailList[i].workcost ? data.detailList[i].workcost.toFixed(2) : '0.00'\r\n            data.detailList[i].dealwithcost = data.detailList[i].dealwithcost ? data.detailList[i].dealwithcost.toFixed(2) : '0.00'\r\n            data.detailList[i].pricewithtax = data.detailList[i].pricewithtax ? data.detailList[i].pricewithtax.toFixed(2) : '0.00'\r\n            data.detailList[i].pricenotax = data.detailList[i].pricenotax ? data.detailList[i].pricenotax.toFixed(2) : '0.00'\r\n          }\r\n          this.tableData = data.detailList\r\n\r\n          for (let i = 0; i < res.data.taxratelist.length; i++) {\r\n            res.data.taxratelist[i].text = res.data.taxratelist[i].value + '%'\r\n          }\r\n          this.options = res.data.taxratelist\r\n\r\n          if (obj && obj.refresh) {\r\n            this.$message.success('刷新成功')\r\n          }\r\n        } else {\r\n          this.$message.success(res.data.data)\r\n        }\r\n\r\n        this.throttles.refresh = true\r\n      })\r\n    },\r\n    ClickImOutport() {\r\n      this.isShowImport = true\r\n    },\r\n    rowBackground(row, index) {\r\n      if (row.statu === '已作废') {\r\n        return 'rowBgColor-d'\r\n      }\r\n      if (index % 2) {\r\n        return 'rowBgColor'\r\n      }\r\n    },\r\n    BlurPricewithtax(event, index) {\r\n      this.tableData[index].pricewithtax = event.target.value && parseFloat(parseFloat(event.target.value).toFixed(2))\r\n    },\r\n    BlurMaterialcost(event, index) {\r\n      this.tableData[index].materialcost = event.target.value && parseFloat(event.target.value).toFixed(2)\r\n    },\r\n    BlurWorkcost(event, index) {\r\n      this.tableData[index].workcost = event.target.value && parseFloat(event.target.value).toFixed(2)\r\n    },\r\n    BlurDealwithcost(event, index) {\r\n      this.tableData[index].dealwithcost = event.target.value && parseFloat(event.target.value).toFixed(2)\r\n    },\r\n    KeydownNumber(event) {\r\n      let code = event.keyCode\r\n      if (!(code >= 48 && code <= 57) && !(code >= 96 && code <= 105) && code != 110 && code != 190 && code != 8 && code != 37 && code != 39) {\r\n        event.returnValue = false\r\n      } else {\r\n        event.returnValue = true\r\n      }\r\n    }\r\n   },\r\n  computed: {},\r\n  watch: {}\r\n}\r\n</script>\r\n\r\n<style lang=\"stylus\">\r\n    .enquirySheetDetail-container\r\n        padding: 40px 30px 30px 30px\r\n        background-color: #fff\r\n        min-width: 1260px\r\n        .el-dialog--small\r\n           max-width: 620px\r\n        .el-input-number\r\n            width: auto\r\n        .w90\r\n          width: 90px\r\n        .icon-slide\r\n            display:inline-block\r\n            width:79px\r\n            height:20px\r\n            background-size:100% 100%\r\n            cursor:pointer\r\n            position: absolute\r\n            text-align:center\r\n            line-height:20px\r\n            font-size:14px\r\n            color:#bfcbd9\r\n            background:url(/static/icon/icon-slide-bg.png) 0 0 no-repeat\r\n            &:hover\r\n                color:#20a0ff\r\n            i\r\n                font-size: 12px\r\n        .icon-silde-arrow\r\n            margin-left: 5px\r\n            transform: rotateZ(-90deg)\r\n        .icon-silde-down\r\n            top: 8px\r\n            left: 50%\r\n        .icon-silde-up\r\n            left:50%\r\n            bottom:-19px\r\n            transform:translateX(-50%)\r\n\r\n        .header-panel\r\n            .header-title\r\n                height: 62px\r\n                padding-left: 20px\r\n                border: 1px solid #e5e5e5\r\n                background-color: #f8f8f8\r\n                .urgent\r\n                    display: inline-block\r\n                    font-size: 12px\r\n                    width: 32px\r\n                    height: 22px\r\n                    line-height: 22px\r\n                    margin: 0 20px\r\n                    border-radius: 5px\r\n                    text-align: center\r\n\r\n                .urgent-A\r\n                    color: #fff\r\n                    background-color: #FF4949\r\n\r\n            .header-BtnGroup\r\n                margin: 30px 0 \r\n                .el-button\r\n                    width: 80px\r\n                    height: 30px\r\n                    line-height: 30px\r\n                    padding: 0\r\n\r\n        .tabs-box\r\n            // margin-right: 46px\r\n            position:relative\r\n            padding: 20px\r\n            border: 1px solid #d1dbe5\r\n            border-radius: 4px\r\n            background-color: #f8f8f8\r\n            overflow: hidden\r\n            box-shadow: 0 2px 4px 0 rgba(0,0,0,.12), 0 0 6px 0 rgba(0,0,0,.04)\r\n            margin: 0 2px 4px 0\r\n            overflow: visible\r\n\r\n            .tabs-box-card\r\n                background-color:#f8f8f8\r\n                margin: 0 2px 4px 0\r\n            .info-box.info-box-lh .info-lists .info-list\r\n                line-height:25px\r\n            .info-box\r\n                .info-lists\r\n                    overflow:hidden\r\n                    width:100%\r\n                    min-width:1120px\r\n                    .info-list\r\n                        float:left\r\n                        font-size:0\r\n                        line-height:22px\r\n                        &:nth-child(1)\r\n                            width:310px\r\n                        &:nth-child(2)\r\n                            width:336px\r\n                        &:nth-child(3)\r\n                            width:336px\r\n                        &:nth-child(4)\r\n                            max-width:300px\r\n                        span\r\n                            font-size:14px\r\n                            color:#5e6d82\r\n          .content-panel        \r\n            .info-title  \r\n              height: 56px\r\n              line-height: 56px\r\n              border-bottom : 1px solid #d1dbe5\r\n              margin-bottom: 30px \r\n              font-size: 16px\r\n              span\r\n                width: 130px\r\n\r\n            .rowBgColor\r\n              background-color: #f8f8f8\r\n            .rowBgColor-d\r\n              background-color: #e9e9e9\r\n            // .baseInfo\r\n            .detailInfo\r\n              .el-table\r\n                .rateColumn\r\n                  .cell\r\n                    padding: 0\r\n                    input\r\n                      border: none\r\n                      text-align: center\r\n                .freightColumn\r\n                .sequenceColumn\r\n                  .cell\r\n                    padding: 0\r\n                .remarkColumn\r\n                  textarea\r\n                    height: 36px    \r\n              .center-input\r\n                input      \r\n                  text-align: center\r\n                  \r\n          .el-pagination__sizes\r\n              margin-left: 15px\r\n\r\n        .slide-fade-enter-active \r\n          transition: all 0.5s ease\r\n        \r\n        .slide-fade-leave-active \r\n          transition: all 0.5s cubic-bezier(1.0, 0.5, 0.8, 1.0)\r\n        \r\n        .slide-fade-enter, .slide-fade-leave-to\r\n          transform: translateX(10px)\r\n          opacity: 0   \r\n        .el-dialog--tiny\r\n          top: auto!important\r\n          width: auto\r\n        .dialogContent  \r\n          width: 408px\r\n          border: 1px solid #e5e5e5\r\n        .required-input  \r\n          background-color: #FFF3F1\r\n          input\r\n            background-color: #FFF3F1\r\n        .el-dialog__wrapper\r\n          display: flex    \r\n          align-items: center\r\n          justify-content: center\r\n</style>\r\n\n\n\n// WEBPACK FOOTER //\n// enquirySheetDetail.vue?7d9cb16c","<template>\r\n    <div class=\"enquirySheetDetailWindow-container\" @click=\"ClickDiv\">\r\n        <div class=\"panel-contant\" @click.stop=\"\" v-if=\"tableData.length > 0\">\r\n            <div class=\"title pd-l20 mg-b40\">{{tableData[i].productcode}}</div>\r\n            <div class=\"body pd-l20\">\r\n    \r\n                <el-button type=\"primary\" class=\"button\" @click=\"Save()\">保存</el-button>\r\n                <el-button class=\"button\" @click=\"cancel()\">取消</el-button>\r\n    \r\n                <h4 class=\"mg-b30 mg-t40\">询价信息</h4>\r\n                <div class=\"enquiryInfo ai-center spanColor\">\r\n                    <div>\r\n                        <span>产品编码：</span>{{tableData[i].productcode}}</div>\r\n                    <div>\r\n                        <span>品牌：</span>{{tableData[i].productbrand}}</div>\r\n                    <div>\r\n                        <span>产品型号/SKU：</span>{{tableData[i].productmodel}}</div>\r\n                    <div>\r\n                        <span>产品名称：</span>{{tableData[i].productname}}</div>\r\n                    <div>\r\n                        <span>附件名/图纸号：</span>{{tableData[i].attachment}}</div>\r\n                    <div>\r\n                        <span>询价单位：</span>{{tableData[i].unit}}</div>\r\n                    <div>\r\n                        <span>询价数量：</span>{{tableData[i].orderamount}}</div>\r\n                </div>\r\n                <h4 class=\"mg-b20\">报价信息</h4>\r\n                <div class=\"spanColor span100 ai-center\">\r\n                    <div class=\"mg-b10\">\r\n                        <span class=\"inblock w94\">是否能做\r\n                            <!-- <i class=\"Danger\">*</i> -->\r\n                        </span>\r\n                        <el-checkbox v-model=\"tableData[i].iscando\">能做</el-checkbox>\r\n                    </div>\r\n                    <div class=\"mg-b10\">\r\n                        <span class=\"inblock w100\">最小订购量</span>\r\n                        <el-input-number v-model=\"tableData[i].minamount\" :min=\"0\" :max=\"9999\" :debounce=\"0\" :step=\"1\"></el-input-number>\r\n                    </div>\r\n                    <div class=\"mg-b10\">\r\n                        <span class=\"inblock w94\">货期（天）\r\n                             <i class=\"Danger\"v-show=\"tableData[i].iscando\">*</i> \r\n                        </span>\r\n                        <el-input-number v-model=\"tableData[i].deliverytime\" :min=\"0\" :max=\"9999\" :debounce=\"0\" class=\"center-input\" :class=\"{'required-input': tableData[i].iscando && !tableData[i].deliverytime ? true : false }\"></el-input-number>\r\n                    </div>\r\n                    <div class=\"mg-b10\">\r\n                        <span class=\"inblock w100\">是否包含运费</span>\r\n                        <el-checkbox v-model=\"tableData[i].issendcost\">包含</el-checkbox>\r\n                    </div>\r\n                    <div class=\"mg-b10\">\r\n                        <span class=\"inblock w94 va-top lh38\">未税单价</span>\r\n                        <span class=\"inblock w280\">\r\n                             <el-input placeholder=\"请输入内容\" :value=\"(parseFloat(this.tableData[this.i].materialcost) + parseFloat(this.tableData[this.i].workcost) + parseFloat(this.tableData[this.i].dealwithcost)).toFixed(2)\" :disabled=\"true\" style=\"margin-bottom: 3px\"></el-input> \r\n                            <span>系统默认未税单价=材料费+加工费+表面处理</span>\r\n                        </span>\r\n                    </div>\r\n                </div>\r\n                <div class=\"spanColor ai-center bgdiv mg-b20 relative\">\r\n                    <div class=\"mg-b10\">\r\n                        <span class=\"inblock w94\">材料费</span>\r\n                         <input type=\"text\" v-model=\"tableData[i].materialcost\" placeholder=\"请输入单个产品的材料费\" @blur=\"BlurMaterialcost($event)\"  @keydown=\"KeydownNumber($event)\" class=\"w280 el-input__inner\"/>  \r\n                    </div>\r\n                    <div style=\"width:415px\">\r\n                        <span class=\"inblock w100 text-center\">加工费</span>\r\n                          <input type=\"text\" v-model=\"tableData[i].workcost\" placeholder=\"请输入单个产品的加工费\" @blur=\"BlurWorkcost($event)\"  @keydown=\"KeydownNumber($event)\" class=\"w280 el-input__inner\"/>  \r\n                    </div>\r\n                    <div