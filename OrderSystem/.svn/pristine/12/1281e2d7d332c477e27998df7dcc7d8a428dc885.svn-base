n            <el-form-item label='主营产品' class=\"w442\" prop='mainproduct'>\r\n              <el-input type='textarea' v-model='IndustryData.mainproduct' placeholder='多个主营产品时，用，隔开'  :autosize=\"{ minRows: 1, maxRows: 4}\" class='info-textarea' style='width: 260px'></el-input>\r\n            </el-form-item>\r\n            <el-form-item label='竞争对手'>\r\n              <el-input type='textarea' v-model='IndustryData.competitor'  placeholder='多个竞争对手时，用，隔开' :autosize=\"{ minRows: 1, maxRows: 4}\" class='info-textarea' style='width: 260px'></el-input>\r\n            </el-form-item>\r\n          </div>\r\n\r\n          <el-form-item>\r\n            <el-button type='primary' @click='SaveIndustryInfo'>保存</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n    </div>\r\n    <div class='supplier-info-box'>\r\n      <h3 ref='business'>商务信息</h3>\r\n      <div class='supplier-warper'>\r\n        <v-Business :BusinessData='BusinessData' :BusinessOptions='BusinessOptions'></v-Business>\r\n      </div>\r\n    </div>\r\n    <div class='supplier-info-box'>\r\n      <h3 ref='financial'>业务信息</h3>\r\n      <div class='supplier-warper'>\r\n        <v-Financial :custbusinesslist='custbusinesslist'></v-Financial>\r\n      </div>\r\n    </div>\r\n    <div class='supplier-info-box'>\r\n      <h3 ref='linkname'>联系人</h3>\r\n      <div class='supplier-warper'>\r\n        <v-Contact :custcontactlist='custcontactlist'></v-Contact>\r\n      </div>\r\n    </div>\r\n    <div class='supplier-info-box'>\r\n      <h3 ref='address'>地址</h3>\r\n      <div class='supplier-warper'>\r\n          <v-address :AddressTableData=\"AddressTableData\" :goTop=\"goTop\"></v-address>\r\n      </div>\r\n    </div>\r\n    <v-backtop :scollTop=\"scollTop\"></v-backtop>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Information from './Information'\r\nimport Business from './Business'\r\nimport Contact from './Contact'\r\nimport Financial from './Financial'\r\nimport address from './addres