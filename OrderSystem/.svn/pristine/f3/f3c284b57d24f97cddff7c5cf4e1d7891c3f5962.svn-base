<script>
	import api from '../../api/api'
	import base from './order'
	import foot from './footer'
	console.log(foot)
	export default {
    data() {
      return {
//筛选区域
        form: {
          productmodel: '',
          productname:'',
          clientname:'',
          outorderid:'',
          inorderid:'',
          ispicture:'',
          createtime: '',
          deliverydatetime:'',
          orderitemstatus:'',
          technologyname:'',
          technologystatus:''
        },
        show: false,
			//我的查询
				mysearch_show:false,
				search_txt:"我的查询",
				searchList:[{message:"我的查询"},{message:"查询条件一"},{message:"查询条件二"},{message:"查询条件三ddddddddddddddddddddddddddddddddddddddd"}],
//功能列表区
			//自定义下拉框数据
        optionData: [],
        checkedArr:[],
        defaultProps: {
          children: 'children',
          label: 'label'
        },
        showOption:false,
      //分页
      	page_sizes:[10,20,30,50,100,200,500,1000],
      //导入订单
      	order_box:false,
//表格
      	tableData3: [],
        listLoading:false,
        multipleSelection: [],    //存储选中的行
        pickerOptions: {
          disabledDate(time) {
            return time.getTime() < Date.now() - 8.64e7;
          }
        },
        orderDate:"",
//保存查询标题
        titleForm: {
        	title:''
        },
        dialogFormVisible:false
      }
    },
    computed:{
    	tableData4 () {
    		if(this.tableData3.length==0){
    			return []
    		}else{
    			let arr = this.tableData3
	    		for(let i = 0;i<arr.length;i++){
	    			arr[i].createtime =  arr[i].createtime.split(" ")[0]   //去掉时分秒
	    			arr[i].totle = arr[i].price * arr[i].orderamount
	    			arr[i].dateDiff = base.checkDateDiff(arr[i])
	    			let lasttime = arr[i].lasttime.split(':') 	//去掉秒
	    					lasttime.pop()
	    			arr[i].lasttime = lasttime.join(":")
	    			let per = Math.round(arr[i].qualifiedamount / arr[i].orderamount * 10000) / 100.00
	    			arr[i].completePercent = isNaN(per)? '0': per + "%"
	    		}
    			return arr
    		}
    	}
    },
    methods: {
//头部筛选
		//查询
      onSubmit() {
        console.log('submit!')
        this.getOrderList()
      },
		//重置
      onRest(formName) {
      	this.$refs[formName].resetFields()
      },
		//保存
      onSave() {
      	this.dialogFormVisible = false
      	if (this.titleForm.title === '') {
      		return false
      	}else {
      		console.log('保存成功'+this.titleForm.title)
      	}
      },
		//我的查询
			mySearch(keyword){
				this.mysearch_show = false
				this.search_txt = keyword
				console.log(keyword)
			},
//功能区
		//分页
			handleSizeChange(val){
				 console.log(`当前页: ${val}`);
			},
      handleCurrentChange(val) {
        console.log(`当前页: ${val}`);
      },
//表格事件
			//select
			handleSelectionChange(val) {
				console.log(val)
				this.multipleSelection = val;
			},
			//输入框
			Blur(id) {
				console.log(id)
				console.log(this.tableData3)
			},
			
//数据请求
			getOrderList() {
				var _t = this
				this.listLoading = true
				//登录接口
				let data = {}
				let userInfo = JSON.parse(sessionStorage.getItem('userInfo'))
				let sessionid = sessionStorage.getItem('token')
				
				data.companyid = userInfo.companyId
				data.factoryid = userInfo.factoryId
				data.sessionid = sessionid
	    	api.getorders(data)
	      	.then(res => {
	         	if(res.code==200){
	         		_t.listLoading = false
			    		_t.tableData3= res.data
			    	}else{
		    			_t.$alert(data.errorMessage, '错误提示', {
		          	confirmButtonText: '确定',
		        	})
			    	}
	      })
	      .catch(error => {
      		_t.listLoading = false
          _t.$alert(error,'错误提示', {
	          	confirmButtonText: '确定',
	        })
	      })
			},
			checkList(data) {
				var checkArr = []
				for(let i=0; i<data.length; i++){
					if(data[i].checked === "true"){
						checkArr.push(data[i].id)
					}
					if(data[i].children !=undefined && data[i].children.length>0){
						for(let j=0; j<data[i].children.length; j++){
							if(data[i].children[j].checked === "true"){
									checkArr.push(data[i].children[j].id)
							}
						}
					}
				}
				
				this.optionData = data
				this.checkedArr = checkArr
			}
    },
    created() {
    	this.getOrderList()
    },
    components:{
    	'v-foot':foot
    }
  }
</script>

