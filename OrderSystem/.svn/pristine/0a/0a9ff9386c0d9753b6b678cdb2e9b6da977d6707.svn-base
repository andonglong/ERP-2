.detail-box
	padding:40px 0 40px 46px
	.box-card
		background-color:#f8f8f8
		margin-right: 46px
		.el-breadcrumb
			font-size:14px
			color:#5e6d82
			.el-breadcrumb__item:last-child
				font-size:16px
			.el-breadcrumb__item:last-child .el-breadcrumb__item__inner, .el-breadcrumb__item:last-child .el-breadcrumb__item__inner a, .el-breadcrumb__item:last-child .el-breadcrumb__item__inner a:hover, .el-breadcrumb__item:last-child .el-breadcrumb__item__inner:hover
				color:#5e6d82
		.nav
			.el-breadcrumb__separator
				margin: 0 15px 0 8px
			.el-breadcrumb
				line-height: 22px
	.main
		margin-top:30px
		background-color:#fff
		.el-tabs__item
				padding:0
				width:120px
				text-align:center
				font-size:16px
				.icon-message
					background-color: #ff4949
					border-radius: 10px
					color: #fff
					font-size: 12px
					display: inline-block
					font-style: inherit
					height: 18px
					line-height: 18px
					padding:0 6px
					text-align:center
					white-space: nowrap
					border: 1px solid #fff
					vertical-align: top
					margin-left: -1px
					margin-top: 10px
		.btn-box
			position:relative
			margin:0 46px 40px 0
			button
				width:80px
				margin:0
				height:30px
				line-height:30px
				padding:0
				text-align:center
				&:nth-child(2)
					width:110px
					margin:0 20px
		.el-tabs__item
			font-size:16px
			line-height:56px
			height:56px
		.el-tabs__header
			margin-bottom:30px
		.icon-slide
			display:inline-block
			width:79px
			height:20px
			background-size:100% 100%
			cursor:pointer
			position: absolute
			text-align:center
			line-height:20px
			font-size:14px
			color:#bfcbd9
			background:url(/static/icon/icon-slide-bg.png) 0 0 no-repeat
			&:hover
				color:#20a0ff
			i
				font-size: 12px
		.icon-silde-arrow
			margin-left:5px
			transform: rotateZ(-90deg)
		.icon-silde-down
			top: 8px
			left: 50%
		.icon-silde-up
			left:50%
			bottom:-15px
			transform:translateX(-50%)
		.tabs-box
			margin-right: 46px
			position:relative
			.tabs-box-card
				background-color:#f8f8f8
				margin: 0 2px 4px 0
				min-width:1228px
			.info-box.info-box-lh .info-lists .info-list
				line-height:25px
			.info-box
				.info-lists
					overflow:hidden
					width:100%
					min-width:1120px
					.info-list
						float:left
						font-size:0
						line-height:22px
						&:nth-child(1)
							width:310px
						&:nth-child(2)
							width:336px
						&:nth-child(3)
							width:336px
						&:nth-child(4)
							width:150px
						span
							font-size:14px
							color:#5e6d82
				.info-lists.info-lists-last
					.info-list
						width:70%
		.main-tabs
			margin-top:20px
			.el-tabs__header
				margin-right:46px
			.el-tabs__content
				overflow:inherit
			.info-border
				border:1px solid #d1dbe5
				border-radius:5px
				&:hover
					border-color:#0189ff
			.info-pos
				display:inline-block
				position:relative
				font-size:0
				margin-right:20px
				.upload-btn
					width:88px
					height:30px
					vertical-align: middle
					padding:0
					line-height:30px
					margin-left:20px
					&:nth-child(1)
						margin-left:0px
				.supplier-btn
					width:104px
					height:30px
					padding:0
					line-height:30px
					border:0
					border-radius:5px 0 0 5px
					vertical-align: middle
				.supplier-tip-btn
					width:30px
					height:30px
					border:0
					border-radius:0 5px 5px 0
					margin-left:0
					text-align:center
					vertical-align: middle
					margin:0
					padding:0
				.info-por
					position:absolute
					width:132px
					border:1px solid #d1dbe5
					top:35px
					left:0
					text-align:center
					z-index:99
					background-color:#fff
					li
						font-size:14px
						width:100%
						height:30px
						line-height:30px
						margin:4px 0
						background-color:#e4e8f1
						color:#1f2d3d
						cursor: pointer
						&:hover
							background-color:#20a0ff
							color:#fff
			.btn-box-second
				.upload-btn
					width:124px
				.add-btn
					width:80px
					height:30px
					vertical-align: middle
					padding:0
					line-height:30px
					margin-left:20px
			.table-box
				.el-table__header-wrapper .el-table__header .cell
					text-align:center
				.info-row-bg
					background-color:#f8f8f8
				.positive-row-bg
					background-color:#fff
				.c20a0ff
					cursor:pointer
				.el-table th
					background-color:#eff2f7
				.el-table__header-wrapper .el-table__header .cell,
				.el-table__fixed-header-wrapper .el-table__header .cell
					line-height:70px
				.el-table .cell
					line-height:54px
				.table-input .el-input>input
					border-radius:3px
					background-color:#fff
					text-align:center
					color:#666
					padding:0
				.table-input.el-textarea
					vertical-align: middle
				.el-input-number .el-input
					width:initial
					margin: 0 10px
					&:focus
						border:1px solid #20a0ff
				.el-input-number
					width:100%
				.el-table .cell
					padding-left:0
					padding-right:0
					/*text-align:center*/
				.marginLeft
					margin-left:20px
				.icon-dot
					&:before
						content:''
						display:inline-block
						width:7px
						height:7px
						border-radius:50%
						margin-right:7px
				.icon-statu-a:before
					background-color:#ff4949
				.icon-statu-b:before
					background-color:#13ce66
				.icon-statu-c:before
					background-color:#20a0ff
				.icon-statu-d:before
					background-color:#999999
				.duty-select
					.el-input__icon+.el-input__inner
						padding: 0
						text-align: center
						padding-right: 15px
						border:0
						background-color:transparent
	.tipbox
		.dialog-upload
			.el-dialog--small
				width:620px
		.dialog-title
			position:relative
			font-size:16px
			line-height:24px
			color:#1f2d3d
			.dialog-title-txt
				vertical-align: middle
			.icon-title-statu
				font-size:21px
				margin-right:10px
				vertical-align: middle
			.el-icon-warning
				font-size:21px
				margin-right:10px
				vertical-align: middle
				background-color:#fff
				color:#108ee9
			.detail-btn
				position:absolute
				width:65px
				left:30px
				bottom:-30px
				cursor:pointer
				color:#5e6d82
			.detail-btn-color
				color:#108ee9
		.dialog-content
			font-size:14px
			line-height:22px
			color:#5e6d82
			.dialog-content-border
				padding:10px 0 10px 30px
				border:1px solid #e5e5e5
				background-color:#f8f8f8
				max-height: 300px
				overflow: auto
			.quotedetail-fail
				.quotedetail-fail-item
					.detail-item
						display:block
						margin-left:10px
					.quotedetail-fail-t
						display:inline-block
						width:75px
		.el-dialog--tiny
			min-width:410px
