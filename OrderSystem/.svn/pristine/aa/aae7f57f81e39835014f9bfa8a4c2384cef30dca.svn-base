otedetail-fail-item\" v-for=\"(item, index) in SuccessList2\">{{index+1}}、“{{item.name == null ? '--': item.name}}”\r\n                  <span v-for=\"(list, i) in item.detail\" class=\"detail-item txt-hid\">{{list.text}}</span>\r\n                </p>\r\n                <p>失败：</p>\r\n                <p class=\"quotedetail-fail-item\" v-for=\"(item, index) in FailList\">{{index+1}}、“{{item.name == null ? '--': item.name}}” , {{item.message}}\r\n                  <span v-for=\"(list, i) in item.detail\" class=\"detail-item txt-hid\">{{list.text}}</span>\r\n                </p>\r\n              </div>\r\n            </div>\r\n          </el-collapse-transition>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n\t\t\t    <el-button type=\"primary\" @click=\"dialogVisible2 = false\">确 定</el-button>\r\n\t\t\t  </span>\r\n    </el-dialog>\r\n  </div>\r\n</div>\n</template>\r\n\r\n<script>\nimport api from 'api/request'\r\nimport $util from 'common/js/util.js'\r\nexport default {\r\n  data () {\r\n    return {\r\n      form: {\r\n        companyid: '',\r\n        factoryid: '',\r\n        willorderid: '',\r\n        quotationid: '',\r\n        status: '',\r\n        inquiryData: '',\r\n        endinquirytime: null,\r\n        startinquirytime: null,\r\n        engineer: '',\r\n        buyer: '',\r\n        productmodel: '',\r\n        pageindex: 1,\r\n        pagesize: 10\r\n      },\r\n      statuOptions: [{\r\n        value: '',\r\n        label: '全部'\r\n      }, {\r\n        value: '0',\r\n        label: '待报价'\r\n      }, {\r\n        value: '1',\r\n        label: '已向供应商询价'\r\n      }, {\r\n        value: '2',\r\n        label: '供应商已报价'\r\n      }, {\r\n        value: '3',\r\n        label: '已筛选供应商报价'\r\n      }, {\r\n        value: '4',\r\n        label: '已确认建议售价'\r\n      }, {\r\n        value: '5',\r\n        label: '已作废'\r\n      }],\r\n      show: false,\r\n      activeName: 'first',\r\n\r\n      tableData: [],\r\n      filters: [{\r\n        value: '0',\r\n        text: '待报价'\r\n      }, {\r\n        value: '1',\r\n        text: '已向供应商询价'\r\n      }, {\r\n        value: '2',\r\n        text: '供应商已报价'\r\n      }, {\r\n        value: '3',\r\n        text: '已筛选供应商报价'\r\n      }, {\r\n        value: '4',\r\n        text: '已确认建议售价'\r\n      }, {\r\n        value: '5',\r\n        text: '已作废'\r\n      }],\r\n      filters2: [],\r\n      defaultSort: {\r\n        prop: 'date',\r\n        order: 'descending'\r\n      },\r\n      count1: 0,\r\n      count2: 0,\r\n\r\n      listLoading: false,\r\n      multipleSelection: [],\r\n\r\n      // 分页\r\n      page_sizes: [10, 20, 30, 50, 100, 200, 500, 1000],\r\n      Total: 100, // 总条数\r\n\r\n      dialogVisible: false,\r\n      dialogVisible2: false,\r\n      iconTitle: true, // 提醒供应商报价返回状态\r\n      quoteDetail: false,\r\n      quoteDetail2: false,\r\n      TipTotal: '',  // 全部成功总数\r\n      TipTotal2: '',\r\n      SuccessList: [], // 全部成功数组\r\n      SuccessList2: [], // 部分成功数组\r\n      FailList: []\r\n    }\r\n  },\r\n  methods: {\r\n    inquiryDataChange (val) {\r\n      if (!$util.checkDateDaterange(val)) {\r\n        this.$notify({\r\n          title: '警告',\r\n          message: '日期格式错误，请重新输入',\r\n          type: 'warning',\r\n          duration: 1500\r\n        })\r\n        return\r\n      }\r\n      let data = val.split(' - ')\r\n      this.form.startinquirytime = data[0]\r\n      this.form.endinquirytime = data[1]\r\n    },\r\n    // 查询\r\n    search () {\r\n      this.form.pageindex = 1\r\n      this.getWillorders(this.form, '查询成功')\r\n      this.activeName = 'first'\r\n    },\r\n    // 重置\r\n    resetForm (formName) {\r\n      this.form.pageindex = 1\r\n      this.$refs[formName].resetFields()\r\n    },\r\n    // 导航切换\r\n    handleClick (tab, event) {\r\n      this.$refs['form'].resetFields()\r\n      if (tab.index == 0) {\r\n        this.form.status = ''\r\n      } else if (tab.index == 1) {\r\n        this.form.status = '0'\r\n      } else {\r\n        this.form.status = '2'\r\n      }\r\n      this.form.pageindex = 1\r\n      this.getWillorders(this.form, '')\r\n    },\r\n    // table\r\n    handleSelectionChange (val) {\r\n      this.multipleSelection = val\r\n    },\r\n    filterTag (value, row) {\r\n      return row.status === value\r\n    },\r\n    filterTag2 (value, row) {\r\n      return row.engineer === value\r\n    },\r\n    toquotationDetail (id) {\r\n      this.$router.push({\r\n        path: '/Quotation/quotationDetail/' + id\r\n      })\r\n    },\r\n    selectable (row, index) {\r\n      if (row.statusTxt === '已作废') {\r\n        return false\r\n      } else if (row.statusTxt === '待报价') {\r\n        return false\r\n      } else {\r\n        return true\r\n      }\r\n    },\r\n    // 刷新\r\n    handRefresh () {\r\n      this.getWillorders(this.form, '刷新成功')\r\n    },\r\n    // 提醒供应商报价\r\n    handTipSupplier () {\r\n      if (this.multipleSelection.length < 1) {\r\n        this.$notify({\r\n          title: '温馨提示',\r\n          message: '请勾选报价需求单',\r\n          type: 'warning',\r\n          duration: 1500,\r\n          offset: 60\r\n        })\r\n      } else {\r\n        let data = []\r\n        for (var i = 0; i < this.multipleSelection.length; i++) {\r\n          data.push(this.multipleSelection[i].willorderdetailid)\r\n        }\r\n        this.remindsupplier(data)\r\n      }\r\n    },\r\n    // 提醒报价API\r\n    remindsupplier(data) {\r\n      api.willorders.remindsupplier({detailIdList: data}).then(res => {\r\n        if (res.data.code == 200) {\r\n          this.remindData(res.data.data)\r\n          // this.dialogVisible = true\r\n        } else {\r\n          this.$alert('提示失败', '提示')\r\n        }\r\n      })\r\n    },\r\n    // 提醒报价返回数据处理\r\n    remindData(data) {\r\n      let Success = {}\r\n      let Fail = {}\r\n      Success.number = 0   // 成功数\r\n      Success.data = []\r\n      Fail.number = 0   // 失败数\r\n      Fail.data = []\r\n      for (var i = 0; i < data.length; i++) {\r\n        let obj = {name: '', detail: []}\r\n        if (data[i].isSuccess) {\r\n          Success.number++\r\n          obj.name = data[i].data.suppliername\r\n          obj.detail = this.CheckDetail(data[i].data.detailList)\r\n          Success.data.push(obj)\r\n        } else {\r\n          Fail.number++\r\n          obj.name = data[i].data.suppliername\r\n          obj.detail = this.CheckDetail(data[i].data.detailList)\r\n          obj.message = data[i].message\r\n          Fail.data.push(obj)\r\n        }\r\n      }\r\n      if (Success.number == 0) {\r\n        // 全部失败\r\n        this.dialogVisible = true\r\n        this.iconTitle = false\r\n        this.TipTotal = '提醒供应商报价失败'\r\n      } else if (Fail.number == 0) {\r\n        // 全部成功\r\n        this.dialogVisible = true\r\n        this.iconTitle = true\r\n        this.TipTotal = `已成功提醒${Success.number}家供应商报价`\r\n        this.SuccessList = Success.data\r\n      } else {\r\n        // 部分成功部分失败\r\n        this.dialogVisible2 = true\r\n        this.TipTotal2 = `已成功提醒${Success.number}家供应商报价,${Fail.number}家失败`\r\n        this.SuccessList2 = Success.data\r\n        this.FailList = Fail.data\r\n      }\r\n    },\r\n    CheckDetail(arr) {\r\n      let str = ''\r\n      let Arry = []\r\n      for (var i = 0; i < arr.length; i++) {\r\n        arr[i].willorderid = arr[i].willorderid == null ? '无报价单号' : arr[i].willorderid\r\n        arr[i].productcode = arr[i].productcode == null ? '无产品编码' : arr[i].productcode\r\n        str = arr[i].willorderid + ' - ' + arr[i].productcode\r\n        Arry.push({text: str})\r\n      }\r\n      return Arry\r\n    },\r\n    // 列表区\r\n    tableRowClassName (row, index) {\r\n      if (row.statusTxt === '已作废' || row.statusTxt === '待报价') {\r\n        return 'info-row-color'\r\n      } else {\r\n        let className = ''\r\n        for (var i = 0; i < this.multipleSelection.length; i++) {\r\n          if (this.multipleSelection[i] == row) {\r\n            className = 'info-row-9ed'\r\n            break\r\n          }\r\n        }\r\n        return className\r\n      }\r\n    },\r\n    // 分页\r\n    handleSizeChange (val) {\r\n      this.form.pagesize = val\r\n      this.getWillorders(this.form, '')\r\n    },\r\n    handleCurrentChange (val) {\r\n      this.form.pageindex = val\r\n      this.getWillorders(this.form, '')\r\n    },\r\n    // 提示层\r\n    handleClose () {\r\n      this.dialogVisible = false\r\n    },\r\n    handleClose2 () {\r\n      this.dialogVisible2 = false\r\n    },\r\n    quotedStatu (status) {\r\n      if (status === '待报价') {\r\n        return 'icon-statu-a'\r\n      } else if (status === '供应商已报价' || status === '已向供应商询价') {\r\n        return 'icon-statu-b'\r\n      } else if (status === '已筛选供应商报价' || status === '已确认建议售价') {\r\n        return 'icon-statu-c'\r\n      } else {\r\n        return 'icon-statu-d'\r\n      }\r\n    },\r\n    // 获取数据\r\n    getWillorders (parms, bool) {\r\n      let _t = this\r\n      this.listLoading = true\r\n      api.willorders.getwillorders(parms).then(res => {\r\n        if (res.data.code == 200) {\r\n          _t.listLoading = false\r\n          let obj = _t.check_Willorders(res.data.datalist)\r\n          _t.tableData = obj.tableData // 列表\r\n          _t.filters2 = obj.personArr\r\n          _t.Total = res.data.datacount // 分页总数\r\n          if (res.data.statuscount.length > 0) {\r\n            res.data.statuscount.map((value, index, arr) => {\r\n              if (value.status == 0) {\r\n                _t.count1 = value.count\r\n              } else if (value.status == 2) {\r\n                _t.count2 = value.count\r\n              }\r\n            })\r\n          }\r\n          if (bool != '') {\r\n            this.$message.success(bool)\r\n          }\r\n        }\r\n      })\r\n    },\r\n    check_Willorders (data) {\r\n      let personArr = []\r\n      data.map((value, index, arr) => {\r\n        let statusTxt = ''\r\n        value.inquirytime = $util.DateFormat(value.inquirytime, 'yyyy-MM-dd hh:mm:ss')\r\n        if (value.remindtime == null) {\r\n          value.remindtime = '--'\r\n        } else {\r\n          value.remindtime = $util.DateFormat(value.remindtime, 'yyyy-MM-dd hh:mm:ss')\r\n        }\r\n        personArr.push(value.engineer)\r\n        switch (value.status) {\r\n          case '0':\r\n            statusTxt = '待报价'\r\n            break\r\n          case '1':\r\n            statusTxt = '已向供应商询价'\r\n            break\r\n          case '2':\r\n            statusTxt = '供应商已报价'\r\n            break\r\n          case '3':\r\n            statusTxt = '已筛选供应商报价'\r\n            break\r\n          case '4':\r\n            statusTxt = '已确认建议售价'\r\n            break\r\n          case '5':\r\n            statusTxt = '已作废'\r\n            break\r\n        }\r\n        value.statusTxt = statusTxt\r\n      })\r\n      personArr = $util.arrayToHeavy(personArr) // 名字去重\r\n      personArr = this.check_person(personArr) // 格式化名字\r\n      return {\r\n        tableData: data,\r\n        personArr: personArr\r\n      }\r\n    },\r\n    check_person (arr) {\r\n      let newArr = []\r\n      arr.map((value, index, arr) => {\r\n        newArr.push({\r\n          text: value,\r\n          value: value\r\n        })\r\n      })\r\n      return newArr\r\n    }\r\n  },\r\n  computed: {\r\n  },\r\n  created () {\r\n    let userInfo = JSON.parse(sessionStorage.getItem('userInfo'))\r\n    if (userInfo == null) {\r\n      this.$router.push('/login')\r\n    }\r\n    this.form.companyid = userInfo.companyid\r\n    this.form.factoryid = userInfo.factoryid\r\n    let parms = {\r\n      companyid: userInfo.companyid,\r\n      factoryid: userInfo.factoryid\r\n    }\r\n    this.getWillorders(parms, '')\r\n    this.$store.state.adminleftnavnum = '/Quotation/quotation'\r\n  }\r\n}\n</script>\r\n\r\n<style lang=\"stylus\">\r\n\t@import '~common/style/headSearch.styl'\r\n\t.quotation\r\n\t\tbackground-color:#fff\r\n\t\tpadding:0 30px\r\n\t\t.search-box\r\n\t\t\tmin-width:1450px\r\n\t\t\tpadding-top:40px\r\