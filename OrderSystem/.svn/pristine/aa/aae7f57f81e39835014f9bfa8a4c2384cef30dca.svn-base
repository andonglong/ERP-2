ox\">\r\n          <el-upload class=\"upload-file\" ref=\"uploadFile\" name=\"uploaddrawingsheet\" :action=\"uploadActionUrl()\" :headers=\"headersObj\" :data=\"uploadFileData\" :multiple=\"false\"  :auto-upload=\"false\" :file-list=\"uploadFileList\" :on-change=\"uploadFileChange\" :before-upload=\"uploadFileBefore\" :on-progress=\"uploadFileProgress\" :on-success=\"uploadFileSuccess\" :on-error=\"uploadFileError\" :on-remove=\"uploadFileRemove\">\r\n            <el-button type=\"primary\" slot=\"trigger\" class=\"uploadBtn\" @click.native=\"ClickUploadFileBtn\">+ 上传文件</el-button>\r\n            <div class=\"upload-abs\">\r\n              <el-button type=\"primary\" @click=\"publish()\" :disabled=\" this.FilePercentage && FilePercentage ? false : true\">发布</el-button>\r\n            </div>\r\n          </el-upload>\r\n        </div>\r\n      </div>\r\n      <!--endprint-->\r\n      <div class=\"container-line\"></div>\r\n      <div class=\"container-down noprint\">\r\n        <div class=\"upload-record\" v-for=\" (item,index) in drawingData.uploadRecord\">\r\n          <div class=\"date\">\r\n            <span>{{ item.creattime | formatDate }}</span>\r\n          </div>\r\n          <div class=\"info-box\">\r\n            <div class=\"Avatar\">\r\n              <img v-if=\"item.img\" :src=\"item.img\"  class=\"mg-r20\">\r\n              <span v-else class=\"icon iconfont icon-touxiang mg-r20\"></span>\r\n            </div>\r\n            <div class=\"info\">\r\n              <div class=\"name\">{{item.username}}</div>\r\n              <div class=\"fs14 c666 mg-b10 word-break\">{{item.remark}}</div>\r\n              <div class=\"fileIcon\">\r\n                <img src=\"/static/icon/file-icon.png\" >\r\n                \r\n              </div>\r\n              <div>\r\n                <a :href=\"item.drawingsheet\" target=\"_blank\" class=\"c20a0ff word-break\">{{item.drawingsheet | getImgName }}</a>\r\n              </div>\r\n            </div>\r\n          </div>\