ber(event.target.value).toFixed(2) : '0.00'\r\n            this.pricenotaxComputed()\r\n            this.pricewithtaxComputed()\r\n            this.taxamountComputed()\r\n        },\r\n        BlurDealwithcost() {\r\n            this.tableData[this.i].dealwithcost = event.target.value ? Number(event.target.value).toFixed(2) : '0.00'\r\n            this.pricenotaxComputed()\r\n            this.pricewithtaxComputed()\r\n            this.taxamountComputed()\r\n        },\r\n        BlurPricewithtax(event) {\r\n            this.tableData[this.i].pricewithtax = event.target.value ? Number(event.target.value).toFixed(2) : '0.00'\r\n            this.taxamountComputed()\r\n        },\r\n        KeydownNumber(event) {\r\n            let code = event.keyCode\r\n            if (!(code >= 48 && code <= 57) && !(code >= 96 && code <= 105) && code != 110 && code != 190 && code != 8 && code != 37 && code != 39) {\r\n                event.returnValue = false\r\n            } else {\r\n                event.returnValue = true\r\n            }\r\n        },\r\n        Save() {\r\n            if (!this.throttles.save) { return }\r\n            this.throttles.save = false\r\n\r\n            this.$emit('update:tableData', this.tableData)\r\n\r\n            let copeData = this.tableData[this.i]\r\n            // 判断是否符合保存条件\r\n            if (copeData.iscando) {\r\n                if (!copeData.pricenotax) {\r\n                    if (!copeData.deliverytime || !parseFloat(copeData.pricewithtax)) {\r\n                        this.$notify.info({\r\n                            title: '提示',\r\n                            message: '能做的产品，请您填写货期和含税单价'\r\n                        })\r\n                        // this.$message.error('能做的产品，请您填写货期和含税单价')\r\n                        this.throttles.save = true\r\n                        return\r\n                    }\r\n                } else {\r\n                    if (!copeData.deliverytime) {\r\n                        this.$notify.info({\r\n                            title: '提示',\r\n                            message: '能做的产品，请您填写货期'\r\n                        })\r\n                        // this.$message.error('能做的产品，请您填写货期')\r\n                        this.throttles.save = true\r\n                        return\r\n                    }\r\n                }\r\n            }\r\n            // 深层拷贝\r\n            copeData = {}\r\n\r\n            for (let j in this.tableData[this.i]) {\r\n                if (j == 'iscando') {\r\n                    copeData[j] = this.tableData[this.i][j] ? '1' : '0'\r\n                } else if (j == 'issendcost') {\r\n                    copeData[j] = this.tableData[this.i][j] ? 1 : 0\r\n                } else {\r\n                    copeData[j] = this.tableData[this.i][j]\r\n                }\r\n            }\r\n            let param = {\r\n                inquiryorderid: this.inquiryorderid,\r\n                detailList: [copeData]\r\n            }\r\n            this.$api.inquiryorder.updateinquiryorder(param).then(res => {\r\n                if (res.data.code == 200) {\r\n                    this.$message.success('保存成功')\r\n                    this.$emit('update:windowShow', false)\r\n                    // this.fetchData()\r\n                } else {\r\n                    this.$message.error(res.data.data ? res.data.data : '服务端错误')\r\n                }\r\n                this.throttles.save = true\r\n            })\r\n        },\r\n        cancel() {\r\n            this.$emit('update:windowShow', false)\r\n        },\r\n        pricenotaxComputed() {\r\n            this.tableData[this.i].pricenotax = (parseFloat(this.tableData[this.i].materialcost) + parseFloat(this.tableData[this.i].workcost) + parseFloat(this.tableData[this.i].dealwithcost)).toFixed(2)\r\n        },\r\n        pricewithtaxComputed() {\r\n            this.tableData[this.i].pricewithtax = (parseFloat(this.tableData[this.i].pricenotax) * (1 + this.tableData[this.i].taxrate / 100)).toFixed(2)\r\n        },\r\n        taxamountComputed() {\r\n            this.tableData[this.i].taxamount = (parseFloat(this.tableData[this.i].pricewithtax) * parseFloat(this.tableData[this.i].orderamount)).toFixed(2)\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"stylus\">\r\n    .enquirySheetDetailWindow-container\r\n      position: fixed\r\n      top: 0\r\n      right: 0\r\n      bottom: 0\r\n      left: 0\r\n      z-index: 2001\r\n      background-color: rgba( 0, 0, 0, 0.5)\r\n      .el-select-dropdown\r\n        z-index: 3005!important\r\n      .w94\r\n        width: 94px\r\n      .w100  \r\n        width: 100px\r\n      .w280  \r\n        width: 280px \r\n      .lh38  \r\n        line-height: 38px\r\n      #square\r\n        position: absolute  \r\n        top: -6px\r\n        left: 30px\r\n        width: 10px\r\n        height: 10px\r\n        border-top: 1px solid #e5e5e5\r\n        border-right: 1px solid #e5e5e5\r\n        transform:rotate(-45deg)\r\n        background-color: #f8f8f8\r\n      .panel-contant\r\n         width: 880px\r\n         height: 100%\r\n         margin: 0 0 0 auto\r\n         background-color: #fff\r\n         .spanColor\r\n            display: inline-flex\r\n            flex-flow: wrap \r\n            >div\r\n              width: 420px\r\n              color: #5e6d82\r\n              >span\r\n                color: #99a9bf\r\n         .title\r\n            height: 54px\r\n            line-height: 54px\r\n            background-color: #f8f8f8\r\n         .enquiryInfo\r\n           >div\r\n             margin-bottom: 25px\r\n         .bgdiv\r\n           margin-left: -9px\r\n           padding: 10px    \r\n           border-radius: 5px\r\n           border: 1px solid #e5e5e5\r\n           background-color: #f8f8f8\r\n</style>\n\n\n// WEBPACK FOOTER //\n// enquirySheetDetailWindow.vue?23077c9e","<template>\r\n  <div class=\"upload-box\">\r\n    <div class=\"upload-step\">\r\n      <h3 class=\"upload-step-title\">第一步：下载模版</h3>\r\n      <p 