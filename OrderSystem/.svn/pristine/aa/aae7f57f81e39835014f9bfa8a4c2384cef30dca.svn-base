   <el-table-column prop=\"telephone\" label=\"联系电话\" min-width=\"196\" align=\"center\">\r\n           <template scope=\"scope\">\r\n                {{scope.row.telephone ? scope.row.telephone : '--'}}\r\n           </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"facktime\" label=\"实际报价时间\" min-width=\"196\" align=\"center\">\r\n          <template scope=\"scope\">\r\n                {{scope.row.facktime ? scope.row.facktime : '--'}}\r\n           </template>\r\n        </el-table-column>\r\n      </el-table>\r\n  \r\n      <div class=\"page-style text-right\">\r\n        <el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" :current-page=\"searchForm.pageindex\" :page-sizes=\"pageSizes\" :page-size=\"searchForm.pagesize\" layout=\"prev, pager, next, sizes, jumper\" :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n  \r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport api from 'api/request.js'\r\nimport $util from 'common/js/util.js'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      enquiryDate: '',\r\n      searchForm: {\r\n        inquiryorderid: '',\r\n        startcreattime: '',\r\n        endcreattime: '',\r\n        orderstatus: '',\r\n        linkname: '',\r\n        pageindex: 1,\r\n        pagesize: 20\r\n      },\r\n      waitQuoteNum: 0,\r\n      QuotedNum: 0,\r\n      activeName: 'all',\r\n      tableData: [],\r\n      open: false,\r\n      filters: [{\r\n        value: '0',\r\n        text: '待报价'\r\n      }, {\r\n        value: '1',\r\n        text: '已报价'\r\n      }, {\r\n        value: '2',\r\n        text: '已作废'\r\n      }],\r\n      // pagination param\r\n      pageSizes: [10, 20, 30, 50, 100, 200, 500, 1000],\r\n      total: 0,\r\n      throttles: {\r\n        search: true,\r\n        refresh: true\r\n      }\r\n    }\r\n  },\r\n  beforeCreate() { },\r\n  created() {\r\n    // 左侧导航\r\n    this.$store.state.adminleftnavnum = '/EnquirySheet/enquirySheet'\r\n    this.fetchData()\r\n  },\r\n  beforeUpdate() { },\r\n  updated() { },\r\n  beforeMount() { },\r\n  mounted() { },\r\n  filters: {},\r\n  computed: {},\r\n  methods: {\r\n    changeDate(val) {\r\n      let data = val.split(' - ')\r\n      this.searchForm.startcreattime = data[0]\r\n      this.searchForm.endcreattime = data[1]\r\n    },\r\n    rowBackground(row, index) {\r\n      if (row.statu === '已作废') {\r\n        return 'rowBgColor-d'\r\n      }\r\n      if (index % 2) {\r\n        return 'rowBgColor'\r\n      }\r\n    },\r\n    filterTag(value, row) {\r\n      return row.orderstatus === value\r\n    },\r\n    quotedStatu(satau) {\r\n      if (satau === '0') {\r\n        return 'icon-statu-a'\r\n      } else if (satau === '1') {\r\n        return 'icon-statu-b'\r\n      } else if (satau === '2') {\r\n        return 'icon-statu-c'\r\n      } else {\r\n        return 'icon-statu-d'\r\n      }\r\n    },\r\n    checkedStatu(satau) {\r\n      if (satau === '0') {\r\n        return '待报价'\r\n      } else if (satau === '1') {\r\n        return '已报价'\r\n      } else if (satau === '2') {\r\n        return '已作废'\r\n      }\r\n    },\r\n    // pagination methods\r\n    handleSizeChange(val) {\r\n      this.searchForm.pageindex = 1\r\n      this.searchForm.pagesize = val\r\n      this.fetchData()\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.searchForm.pageindex = val\r\n      this.fetchData()\r\n    },\r\n    ClickMark(index, row) {\r\n      let param = { inquiryorderid: row.inquiryorderid }\r\n      if (!parseInt(row.ismark)) {\r\n        api.inquiryorder.markbyid(param).then(res => {\r\n          this.tableData[index].ismark = 1\r\n          // this.fetchData()\r\n        })\r\n      } else {\r\n        api.inquiryorder.unmarkbyid(param).then(res => {\r\n          this.tableData[index].ismark = 0\r\n          // this.fetchData()\r\n        })\r\n      }\r\n    },\r\n    // ClickUrgent(index, isurgent) {\r\n    //   this.tableData[index].isurgen