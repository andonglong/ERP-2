            }\r\n            this.tableData = data.datalist\r\n            this.total = data.datacount\r\n            this.waitQuoteNum = data.unreadcount\r\n            if (obj && obj.search) {\r\n              if (param.orderstatus === '0') {\r\n                this.activeName = '0'\r\n              } else if (param.orderstatus === '1') {\r\n                this.activeName = '1'\r\n              } else {\r\n                this.activeName = 'all'\r\n              }\r\n              this.$message.success('查询成功')\r\n            }\r\n            if (obj && obj.refresh) {\r\n              this.$message.success('刷新成功')\r\n            }\r\n          }\r\n        } else {\r\n          this.$message.error('服务端错误')\r\n        }\r\n        this.throttles.search = true\r\n        this.throttles.refresh = true\r\n      })\r\n    }\r\n  },\r\n  watch: {}\r\n}\r\n</script>\r\n\r\n<style lang=\"stylus\">\r\n  .enquirySheet-container\r\n    padding: 40px 30px 30px 30px\r\n    background-color: #fff\r\n\r\n    .search-form\r\n        &:after\r\n          content: \"\";\r\n          display: block;\r\n          height: 0;\r\n          clear: both;\r\n          visibility: hidden;\r\n        .search-form-left\r\n            width: 750px\r\n        .el-form-item\r\n            display: inline-block\r\n            .enquiryNumber\r\n            .Date\r\n            .status\r\n            .man\r\n              width: 260px\r\n            .spreadbtn\r\n              margin-left:20px\r\n            .icon-spread\r\n              margin-left:5px\r\n            .icon-arrow-up\r\n              transform: rotateZ(90deg)\r\n            .icon-arrow-down\r\n              transform: rotateZ(-90deg)\r\n        .search-form-right\r\n          .btnGroup\r\n             .el-form-item__content\r\n                margin-left: 0 !important\r\n    .content\r\n        .el-table__body-wrapper\r\n          min-height: 540px\r\n        .font-color\r\n          color: #20a0ff\r\n          cursor: pointer\r\n  