.willorderInfo-box
	position:fixed
	right:0
	top:0
	bottom:0
	width:880px
	min-width:880px
	background-color:#fff
	z-index:2000
	padding-top:20px
	overflow-y:auto
	.willorderInfo-title
		font-size:14px
		color:#1f2d3d
		font-weight:bold
		margin-left:20px
	.btn-box
		margin:60px 0 40px 20px
		font-size:0
		.btn-w80
			width:80px
			height:30px
			font-size:14px
			padding:0
			line-height:30px
			text-align:center
		.btn-w80+.btn-w80
			margin-left:20px
	.info-item
		margin-bottom:24px
		padding: 0 30px
		border-bottom:1px dashed #eaeaea
		overflow:hidden
		position:relative
		h4
			font-size:14px
			color:#1f2d3d
			font-weight:bold
			margin-bottom:14px
		.left
			float:left
			width:384px
			margin-bottom:10px
		.right
			float:right
			width:385px
			margin-bottom:10px
		.info
			font-size:14px
			color:#99a9bf
			li
				line-height:38px
			.font-color-5
				color:#5e6d82
		.info-item-input
			width:280px
			height:34px
		.info-item-input-number
			width:200px
			height:34px
		.price-tip
			font-size:12px
			color:#99a9bf
			line-height:20px
		&.supplier-box
			padding:0 0 50px 0
			border-bottom:0
			h4
				padding:0 30px
			.left
				padding-left:30px
				margin-bottom:0
			.right
				padding-right:30px
				margin-bottom:0
		.el-form-item
			margin-bottom:10px
	.pricenotax-box
		background-color:#f8f8f8
		border:1px solid #e5e5e5
		border-radius:7px
		overflow:hidden
		margin:0 20px 20px
		padding:10px 8px
		.left
			padding-left:0!important
		.right
			padding-right:0!important
	.clear
		clear:both
