import Vue from 'vue'
import Vuex from 'vuex'

Vue.use(Vuex)

const state = {
    productDetail: {
         tabs: 'first',
         record: {}
    },
    adminleftnavnum: '/Track/order'
}

const mutations = {
    getProductDetailRecord (state, val) {
        state.productDetail.record = 1
    }
}

export default new Vuex.Store({
    state,
    mutations
})
